        <obo:IAO_0000119>NCI Thesaurus: C42656</obo:IAO_0000119>
        <rdfs:seeAlso>NCI Thesaurus: C42656</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://www.OPHPDD.com/ontologies/OPHPDD.owl/OPHPDD_0000024 -->

    <owl:Class rdf:about="http://www.OPHPDD.com/ontologies/OPHPDD.owl/OPHPDD_0000024">
