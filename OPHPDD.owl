        <obo:IAO_0000115>Therapeutically inactive substance that helps to make pills hold their shape upon compression and improve compressibility properties in general.</obo:IAO_0000115>
    </owl:Class>
    


    <!-- http://www.OPHPDD.com/ontologies/OPHPDD.owl/OPHPDD_0000025 -->

    <owl:Class rdf:about="http://www.OPHPDD.com/ontologies/OPHPDD.owl/OPHPDD_0000025">
