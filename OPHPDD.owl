        <rdfs:subClassOf rdf:resource="http://www.OPHPDD.com/ontologies/OPHPDD.owl/OPHPDD_0000020"/>
        <obo:IAO_0000115>Disintegrant excipient is a therapeutically inactive ingredient of a solid pharmaceutical dosage form that helps the form to dissolve within the gastrointestinal tract to ensure full release of the pharmaceutically active material.</obo:IAO_0000115>
        <obo:IAO_0000119>NCI Thesaurus: C42648</obo:IAO_0000119>
        <rdfs:seeAlso>NCI Thesaurus: C42648</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://www.OPHPDD.com/ontologies/OPHPDD.owl/OPHPDD_0000027 -->

